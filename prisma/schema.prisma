generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Event {
  id          String   @id @map("_id") @default(auto()) @db.ObjectId
  title       String
  description String?
  startsAt    DateTime?
  createdAt   DateTime @default(now())
}
